{
  "name": "@danielpickett/theme-generator",
  "version": "0.0.0",
  "private": true,
  "homepage": "http://danielpickett.github.io/theme-generator",
  "types": "dist/types.d.ts",
  "source": "src/index.ts",
  "alias": {
    "src": "./src"
  },
  "browserModern": "dist/index.js",
  "targets": {
    "browserModern": {
      "engines": {
        "browsers": [
          "last 1 Chrome version"
        ]
      }
    }
  },
  "scripts": {
    "start": "rm -rf dist && parcel watch --no-cache",
    "build": "rm -rf dist && parcel build --no-cache"
  },
  "dependencies": {
    "@danielpickett/components": "0.0.0",
    "@danielpickett/types": "0.0.0",
    "@types/chroma-js": "^2.1.4",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "chroma-js": "^2.4.2",
    "classnames": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recoil": "0.7.5"
  },
  "devDependencies": {
    "@parcel/transformer-typescript-tsc": "^2.7.0",
    "parcel": "^2.7.0",
    "typescript": "4.7.4"
  }
}
