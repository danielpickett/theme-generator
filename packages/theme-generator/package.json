{
  "name": "@danielpickett/theme-generator",
  "version": "0.0.0",
  "private": true,
  "homepage": "http://danielpickett.github.io/theme-generator",
  "types": "dist/types.d.ts",
  "source": "src/index.ts",
  "alias": {
    "src": "./src"
  },
  "browserModern": "dist/index.js",
  "targets": {
    "browserModern": {
      "engines": {
        "browsers": [
          "last 1 Chrome version"
        ]
      }
    }
  },
  "scripts": {
    "start": "rm -rf dist && parcel --no-cache",
    "build": "rm -rf dist && parcel build --no-cache"
  },
  "dependencies": {
    "@danielpickett/types": "0.0.0",
    "@types/chroma-js": "^2.1.3",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "chroma-js": "^2.1.1",
    "classnames": "^2.3.1",
    "react": "^18.0.0-rc.1",
    "react-dom": "^18.0.0-rc.1",
    "recoil": "0.6.1",
    "typescript": "^4.1.2"
  }
}
